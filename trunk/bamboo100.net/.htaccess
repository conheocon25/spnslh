Options +FollowSymlinks
RewriteEngine on
 
#---------------------------------------------------------------------------------------------------------------------------
# BASE
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^trang-chu$ 											?cmd=Home [L]
RewriteRule ^ung-dung/co-tuong$ 									?cmd=AppCChess [L]

RewriteRule ^du-an/([a-z0-9_-]+)$ 									?cmd=ProjectDetail&Key=$1 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/tin-tuc$ 							?cmd=ProjectNews&Key=$1 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/tin-tuc/([a-z0-9_-]+)$ 			?cmd=ProjectNews&Key=$1&NKey=$2 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/hinh-anh$ 							?cmd=ProjectAlbum&Key=$1 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/video$ 							?cmd=ProjectVideo&Key=$1 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/video/([a-z0-9_-]+)$ 				?cmd=ProjectVideo&Key=$1&VKey=$2 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/hang-muc$ 							?cmd=ProjectProduct&Key=$1 [L]
RewriteRule ^du-an/([a-z0-9_-]+)/tai-lieu$ 							?cmd=ProjectDocument&Key=$1 [L]

RewriteRule ^tin-tuc$ 												?cmd=News [L]
RewriteRule ^tin-tuc/([a-z0-9_-]+)$ 								?cmd=ReadCategory&Key=$1 [L]
RewriteRule ^tin-tuc/([a-z0-9_-]+)/trang-([a-z0-9_-]+)$ 			?cmd=ReadCategory&Key=$1&Page=$2 [L]
RewriteRule ^tin-tuc/([a-z0-9_-]+)/([a-z0-9_-]+)$ 					?cmd=ReadNews&Key=$2 [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^dang-nhap$ 											?cmd=SigninLoad [L]
RewriteRule ^dang-nhap/exe$ 										?cmd=SigninExe [L]
RewriteRule ^signout$ 												?cmd=SignoutExe [L]

RewriteRule ^object/load/([A-Za-z0-9]+)/([0-9]+)$ 					?cmd=ObjectLoad&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/del/([A-Za-z0-9]+)/([0-9]+)$ 					?cmd=ObjectDel&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/ins/([A-Za-z0-9]+)$ 							?cmd=ObjectIns&ObjectName=$1 [L]
RewriteRule ^object/upd/([A-Za-z0-9]+)$ 							?cmd=ObjectUpd&ObjectName=$1 [L]

RewriteRule ^quan-ly$ 												?cmd=Setting [L]
RewriteRule ^quan-ly/bai-viet$										?cmd=SettingPost [L]
RewriteRule ^quan-ly/bai-viet/([0-9]+)/upd-load$					?cmd=SettingPostUpdLoad&IdPost=$1 [L]
RewriteRule ^quan-ly/bai-viet/([0-9]+)/upd-exe$						?cmd=SettingPostUpdExe&IdPost=$1 [L]
RewriteRule ^quan-ly/bai-viet/([0-9]+)/del-load$					?cmd=SettingPostDelLoad&IdPost=$1 [L]
RewriteRule ^quan-ly/bai-viet/([0-9]+)/del-exe$						?cmd=SettingPostDelExe&IdPost=$1 [L]
RewriteRule ^quan-ly/bai-viet/ins-load$								?cmd=SettingPostInsLoad [L]
RewriteRule ^quan-ly/bai-viet/ins-exe$								?cmd=SettingPostInsExe [L]

RewriteRule ^quan-ly/co-tuong$										?cmd=SettingCChess [L]
RewriteRule ^quan-ly/co-tuong/([0-9]+)/upd-load$					?cmd=SettingCChessUpdLoad&IdCBook=$1 [L]
RewriteRule ^quan-ly/co-tuong/([0-9]+)/upd-exe$						?cmd=SettingCChessUpdExe&IdCBook=$1 [L]
RewriteRule ^quan-ly/co-tuong/([0-9]+)/del-load$					?cmd=SettingCChessDelLoad&IdCBook=$1 [L]
RewriteRule ^quan-ly/co-tuong/([0-9]+)/del-exe$						?cmd=SettingCChessDelExe&IdCBook=$1 [L]
RewriteRule ^quan-ly/co-tuong/ins-load$								?cmd=SettingCChessInsLoad [L]
RewriteRule ^quan-ly/co-tuong/ins-exe$								?cmd=SettingCChessInsExe [L]