Options +FollowSymlinks
RewriteEngine on
 
#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^signin/load$ 			?cmd=SigninLoad [L]
RewriteRule ^signin/exe$ 			?cmd=SigninExe [L]
RewriteRule ^signout/load$ 			?cmd=SignoutLoad [L]
RewriteRule ^signout/exe$ 			?cmd=SignoutExe [L]
RewriteRule ^change/pass/load$ 		?cmd=ChangePassLoad [L]
RewriteRule ^change/pass/exe$ 		?cmd=ChangePassExe [L]

RewriteRule ^object/load/([A-Za-z0-9]+)/([0-9]+)$ 				?cmd=ObjectLoad&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/del/([A-Za-z0-9]+)/([0-9]+)$ 				?cmd=ObjectDel&ObjectName=$1&Id=$2 [L]
RewriteRule ^object/ins/([A-Za-z0-9]+)$ 						?cmd=ObjectIns&ObjectName=$1 [L]
RewriteRule ^object/upd/([A-Za-z0-9]+)$ 						?cmd=ObjectUpd&ObjectName=$1 [L]

#---------------------------------------------------------------------------------------------------------------------------
# SELLING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^app$ 												?cmd=App [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^setting$ 											?cmd=Setting [L]
RewriteRule ^setting/theme/([a-z0-9_-]+)$ 						?cmd=SettingThemeChangeExe&Theme=$1 [L]

RewriteRule ^setting/category/news$ 							?cmd=SettingCategoryNews [L]
RewriteRule ^setting/category/news/page/([0-9]+)$ 				?cmd=SettingCategoryNews&Page=$1 [L]
RewriteRule ^setting/category/news/([0-9]+)$ 					?cmd=SettingNews&IdCategory=$1 [L]
RewriteRule ^setting/category/news/([0-9]+)/page/([0-9]+)$ 		?cmd=SettingNews&IdCategory=$1&Page=$2 [L]

RewriteRule ^setting/project$ 									?cmd=SettingProject [L]
RewriteRule ^setting/project/page/([0-9]+)$ 					?cmd=SettingProject&Page=$1 [L]
RewriteRule ^setting/project/([0-9]+)/document$ 				?cmd=SettingProjectDocument&IdProject=$1 [L]
RewriteRule ^setting/project/([0-9]+)/document/page/([0-9]+)$ 	?cmd=SettingProjectDocument&IdProject=$1&Page=$2 [L]
RewriteRule ^setting/project/([0-9]+)/album$ 					?cmd=SettingProjectAlbum&IdProject=$1 [L]
RewriteRule ^setting/project/([0-9]+)/album/page/([0-9]+)$ 		?cmd=SettingProjectAlbum&IdProject=$1&Page=$2 [L]
RewriteRule ^setting/project/([0-9]+)/video$ 					?cmd=SettingProjectVideo&IdProject=$1 [L]
RewriteRule ^setting/project/([0-9]+)/video/page/([0-9]+)$ 		?cmd=SettingProjectVideo&IdProject=$1&Page=$2 [L]
RewriteRule ^setting/project/([0-9]+)/news$ 					?cmd=SettingProjectNews&IdProject=$1 [L]
RewriteRule ^setting/project/([0-9]+)/news/page/([0-9]+)$ 		?cmd=SettingProjectNews&IdProject=$1&Page=$2 [L]
RewriteRule ^setting/project/([0-9]+)/product$ 					?cmd=SettingProjectProduct&IdProject=$1 [L]
RewriteRule ^setting/project/([0-9]+)/product/page/([0-9]+)$ 	?cmd=SettingProjectProduct&IdProject=$1&Page=$2 [L]

RewriteRule ^setting/customer$ 									?cmd=SettingCustomer [L]
RewriteRule ^setting/customer/page/([0-9]+)$ 					?cmd=SettingCustomer&Page=$1 [L]

RewriteRule ^setting/config$ 									?cmd=SettingConfig [L]
RewriteRule ^setting/config/page/([0-9]+)$ 						?cmd=SettingConfig&Page=$1 [L]

RewriteRule ^setting/user$ 										?cmd=SettingUser [L]
RewriteRule ^setting/user/page/([0-9]+)$ 						?cmd=SettingUser&Page=$1 [L]