RewriteRule ^admin/setting/employee$ 									?cmd=ASettingEmployee [L]
RewriteRule ^admin/setting/employee/page/([0-9]+)$ 						?cmd=ASettingEmployee&Page=$1 [L]

RewriteRule ^admin/setting/store$ 										?cmd=ASettingStore [L]
